<!-- app/views/dinosaurs/_form.html.erb -->

<%= form_for @dinosaur do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :age %>
  <%= f.number_field :age %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit (@dinosaur.new_record? ? "Create" : "Update") %>

<% end %>
